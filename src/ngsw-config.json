{
	"$schema": "../node_modules/@angular/service-worker/config/schema.json",
	"index": "/index.html",
	"assetGroups": [
		{
			"name": "app",
			"installMode": "prefetch",
			"updateMode": "prefetch",
			"resources": {
				"files": [
					"/index.html",
					"/favicon.ico",
					"/favicon.svg",
					"/manifest.webmanifest",
					"/*.js",
					"/*.css"
				]
			}
		},
		{
			"name": "assets",
			"installMode": "prefetch",
			"updateMode": "lazy",
			"resources": {
				"files": ["/font/**", "/svg/**", "/icon-*.{png,svg}"]
			}
		}
	]
}
